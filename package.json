{
  "name": "public-health",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll -no-cache",
    "build-client": "cd client && npm run build",
    "install-client": "cd client && npm install --force",
    "heroku-postbuild": "npm run install-client && npm run build-client",
    "server": "nodemon server --ignore client",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \" npm run client \""
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Patrickyyh/public-health-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Patrickyyh/public-health-app/issues"
  },
  "homepage": "https://github.com/Patrickyyh/public-health-app#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.19.3",
    "concurrently": "^7.4.0",
    "jest": "^29.1.2",
    "mongodb-memory-server": "^8.9.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.0"
  }
}
